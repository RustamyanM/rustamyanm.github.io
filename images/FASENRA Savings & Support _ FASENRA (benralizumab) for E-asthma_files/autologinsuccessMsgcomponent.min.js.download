$(document).ready(function(){try{if(getCookie("loadProfile")){if(CQ_Analytics&&CQ_Analytics.ProfileDataMgr&&CQ_Analytics.ClientContextMgr&&CQ_Analytics.PageDataMgr){CQ_Analytics.ProfileDataMgr.clear();
CQ_Analytics.ClientContextUtils.init("/etc/clientcontext/gdd",CQ_Analytics.PageDataMgr.getProperty("path",true));
CQ.shared.HTTP.clearCookie("loadProfile","/")
}}if(window.AZ&&window.AZ.Env){if(window.AZ.Env.isTieredAccessSession){if(CQ_Analytics&&CQ_Analytics.CCM&&CQ_Analytics.ProfileDataMgr&&CQ_Analytics.ClientContextMgr&&CQ_Analytics.PageDataMgr){if(CQ_Analytics.ProfileDataMgr.getProperty("authorizableId")=="anonymous"){CQ_Analytics.ProfileDataMgr.clear();
CQ_Analytics.CCM.reset();
CQ_Analytics.ClientContextUtils.init("/etc/clientcontext/gdd",CQ_Analytics.PageDataMgr.getProperty("path",true))
}}}}}catch(a){}});
function getCookie(b){var e=b+"=";
var a=document.cookie.split(";");
for(var d=0;
d<a.length;
d++){var f=a[d];
while(f.charAt(0)==" "){f=f.substring(1,f.length)
}if(f.indexOf(e)==0){return f.substring(e.length,f.length)
}}return null
};